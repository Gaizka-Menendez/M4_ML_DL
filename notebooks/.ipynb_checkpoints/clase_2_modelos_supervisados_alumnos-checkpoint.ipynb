    "    feature1_idx: Union[int, str] = 0, \n",
    "    feature2_idx: Union[int, str] = 1, \n",
    "    feature_names: Optional[List[str]] = None, \n",
    "    class_names: Optional[List[str]] = None, \n",
    "    mesh_step_size: float = 0.02, \n",
    "    plot_title: str = \"SVC Decision Boundary\",\n",
    "    figsize: Tuple[int, int] = (10, 8)\n",
    ") -> Tuple[plt.Figure, plt.Axes]:\n",
    "    \"\"\"\n",
    "    Visualiza el hiperplano de decisi칩n (decision boundary) para un modelo SVC con 2 variables seleccionadas.\n",
    "    \n",
    "    Args:\n",
    "        model: Modelo SVC ya entrenado con m칠todo predict\n",
    "        X: Array o DataFrame con todas las caracter칤sticas (features)\n",
    "        y: Array o Series con las etiquetas (target)\n",
    "        feature1_idx: 칈ndice o nombre de la primera caracter칤stica a visualizar\n",
    "        feature2_idx: 칈ndice o nombre de la segunda caracter칤stica a visualizar\n",
    "        feature_names: Lista con los nombres de las caracter칤sticas (opcional)\n",
    "        class_names: Lista con los nombres de las clases (opcional)\n",
    "        mesh_step_size: Tama침o del paso para crear la malla de visualizaci칩n\n",
    "        plot_title: T칤tulo del gr치fico\n",
    "        figsize: Tama침o de la figura como tupla (ancho, alto)\n",
    "        \n",
    "    Returns:\n",
    "        Tuple[plt.Figure, plt.Axes]: Objetos de la figura y eje para posibles personalizaciones adicionales\n",
    "    \"\"\"\n",
    "    from matplotlib.colors import ListedColormap\n",
    "    import matplotlib.pyplot as plt\n",
    "    import numpy as np\n",
    "    \n",
    "    # Convertir DataFrame a NumPy array si es necesario\n",
    "    if hasattr(X, 'iloc'):\n",
    "        # Si se pasan nombres de caracter칤sticas en lugar de 칤ndices\n",
    "        if isinstance(feature1_idx, str):\n",
    "            feature1_idx = X.columns.get_loc(feature1_idx)\n",
    "        if isinstance(feature2_idx, str):\n",
    "            feature2_idx = X.columns.get_loc(feature2_idx)\n",
    "        \n",
    "        # Extraer los nombres de las caracter칤sticas si no se proporcionaron\n",
    "        if feature_names is None and hasattr(X, 'columns'):\n",
    "            feature_names = X.columns.tolist()\n",
    "            \n",
    "        X = X.values\n",
    "        \n",
    "    if hasattr(y, 'values'):\n",
    "        y = y.values\n",
    "    \n",
    "    # Extraer las dos caracter칤sticas a visualizar\n",
    "    X_2d = X[:, [feature1_idx, feature2_idx]]\n",
    "    \n",
    "    # Crear figura\n",
    "    fig, ax = plt.subplots(figsize=figsize)\n",
    "    \n",
    "    # Definir colores para las clases y regiones\n",
    "    n_classes = len(np.unique(y))\n",
    "    colors = ['#1f77b4', '#ff7f0e', '#2ca02c', '#d62728', '#9467bd', \n",
    "              '#8c564b', '#e377c2', '#7f7f7f', '#bcbd22', '#17becf']\n",
    "    \n",
    "    # Definir mapas de color para el scatter plot y para las regiones\n",
    "    cmap_bold = ListedColormap(colors[:n_classes])\n",
    "    cmap_light = ListedColormap([f'#{color[1:]}33' for color in colors[:n_classes]])\n",
    "    \n",
    "    # Determinar l칤mites para el gr치fico\n",
    "    x_min, x_max = X_2d[:, 0].min() - 1, X_2d[:, 0].max() + 1\n",
    "    y_min, y_max = X_2d[:, 1].min() - 1, X_2d[:, 1].max() + 1\n",
    "    \n",
    "    # Crear una malla para visualizar el l칤mite de decisi칩n\n",
    "    xx, yy = np.meshgrid(np.arange(x_min, x_max, mesh_step_size),\n",
    "                         np.arange(y_min, y_max, mesh_step_size))\n",
    "    \n",
    "    # Asegurar que el modelo tiene m칠todo predict\n",
    "    if hasattr(model, 'predict'):\n",
    "        # Preparar los datos de la malla para predecir\n",
    "        mesh_points = np.c_[xx.ravel(), yy.ravel()]\n",
    "        \n",
    "        # Si el modelo fue entrenado con m치s de 2 caracter칤sticas, necesitamos manejar esto\n",
    "        if X.shape[1] > 2:\n",
    "            # Crear datos dummy para las caracter칤sticas no visualizadas\n",
    "            X_mean = np.mean(X, axis=0)\n",
    "            mesh_full = np.zeros((mesh_points.shape[0], X.shape[1]))\n",
    "            for i in range(X.shape[1]):\n",
    "                if i == feature1_idx:\n",
    "                    mesh_full[:, i] = mesh_points[:, 0]\n",
    "                elif i == feature2_idx:\n",
    "                    mesh_full[:, i] = mesh_points[:, 1]\n",
    "                else:\n",
    "                    # Usar la media para las otras caracter칤sticas\n",
    "                    mesh_full[:, i] = X_mean[i]\n",
    "            \n",
    "            # Predecir clases para cada punto de la malla\n",
    "            Z = model.predict(mesh_full)\n",
    "        else:\n",
    "            # Si solo hay 2 caracter칤sticas, predecir directamente\n",
    "            Z = model.predict(mesh_points)\n",
    "        \n",
    "        # Dar forma a los resultados de vuelta a la forma de la malla\n",
    "        Z = Z.reshape(xx.shape)\n",
    "        \n",
    "        # Trazar el l칤mite de decisi칩n\n",
    "        ax.contourf(xx, yy, Z, cmap=cmap_light, alpha=0.8)\n",
    "    \n",
    "    # Trazar los puntos de datos reales\n",
    "    scatter = ax.scatter(X_2d[:, 0], X_2d[:, 1], c=y, cmap=cmap_bold, \n",
    "                         edgecolor='k', s=50, alpha=0.8)\n",
    "    \n",
    "    # Configurar etiquetas y t칤tulo\n",
    "    if feature_names is not None:\n",
    "        ax.set_xlabel(feature_names[feature1_idx])\n",
    "        ax.set_ylabel(feature_names[feature2_idx])\n",
    "    else:\n",
    "        ax.set_xlabel(f'Feature {feature1_idx}')\n",
    "        ax.set_ylabel(f'Feature {feature2_idx}')\n",
    "    \n",
    "    ax.set_title(plot_title)\n",
    "    \n",
    "    # A침adir leyenda si hay nombres de clases\n",
    "    if class_names is not None:\n",
    "        legend1 = ax.legend(scatter.legend_elements()[0], \n",
    "                            class_names,\n",
    "                            loc=\"upper right\", \n",
    "                            title=\"Classes\")\n",
    "        ax.add_artist(legend1)\n",
    "    \n",
    "    # Dibujar vectores de soporte si est치n disponibles\n",
    "    if hasattr(model, 'support_vectors_'):\n",
    "        # Encontrar los vectores de soporte para las 2 dimensiones que estamos visualizando\n",
    "        support_vectors = model.support_vectors_\n",
    "        \n",
    "        # Si tenemos m치s de 2 dimensiones, filtrar solo las 2 que estamos visualizando\n",
    "        if support_vectors.shape[1] > 2:\n",
    "            support_vectors = support_vectors[:, [feature1_idx, feature2_idx]]\n",
    "        \n",
    "        # Dibujar los vectores de soporte\n",
    "        ax.scatter(support_vectors[:, 0], support_vectors[:, 1], \n",
    "                   s=100, linewidth=1, facecolors='none', edgecolors='k',\n",
    "                   label='Support Vectors')\n",
    "        ax.legend()\n",
    "    \n",
    "    # A침adir grid\n",
    "    ax.grid(True, linestyle='--', alpha=0.7)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    return fig, ax"
   ]
